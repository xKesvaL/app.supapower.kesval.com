{
	"name": "starter-svelte",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"npm:dev:*\" --restart-tries 1",
		"dev:svelte": "vite dev",
		"dev:storybook": "storybook dev -p 6006",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"build": "vite build && npm run optimize-images",
		"optimize-images": "image-opti --source ./build/images --target ./build/images",
		"build:vercel": "npm run build && npm run optimize-images:vercel",
		"optimize-images:vercel": "image-opti --source .vercel/output/static/images --target .vercel/output/static/images"
	},
	"devDependencies": {
		"@kesval/design": "^3.0.0",
		"@kesval/image": "^0.1.4",
		"@kesval/image-svelte": "^0.1.4",
		"@storybook/addon-a11y": "^7.2.2",
		"@storybook/addon-docs": "^7.2.2",
		"@storybook/addon-essentials": "^7.2.2",
		"@storybook/addon-interactions": "^7.2.2",
		"@storybook/addon-links": "^7.2.2",
		"@storybook/addon-storysource": "^7.2.2",
		"@storybook/addon-svelte-csf": "^3.0.7",
		"@storybook/blocks": "^7.2.2",
		"@storybook/svelte": "^7.2.2",
		"@storybook/sveltekit": "^7.2.2",
		"@storybook/testing-library": "^0.2.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@total-typescript/ts-reset": "^0.4.2",
		"@types/node": "^20.4.8",
		"@types/nprogress": "^0.2.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.2.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-storybook": "^0.6.13",
		"eslint-plugin-svelte": "^2.30.0",
		"nprogress": "^0.2.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.63.6",
		"storybook": "^7.2.2",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-i18n": "^3.7.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module"
}
