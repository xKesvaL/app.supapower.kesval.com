{
  "name": "starter-svelte",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:dev:*\" --restart-tries 1",
    "dev:svelte": "vite dev --host",
    "dev:storybook": "storybook dev -p 6006",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "build": "vite build && npm run optimize-images",
    "optimize-images": "image-opti --source ./build/images --target ./build/images",
    "build:vercel": "vite build && npm run optimize-images:vercel",
    "optimize-images:vercel": "image-opti -s .vercel/output/static/images -t .vercel/output/static/images -v"
  },
  "devDependencies": {
    "@formkit/auto-animate": "^0.7.0",
    "@kesval/design": "^3.1.0",
    "@kesval/image": "^0.2.0",
    "@kesval/image-svelte": "^0.2.1",
    "@storybook/addon-a11y": "^7.4.0",
    "@storybook/addon-docs": "^7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-storysource": "^7.4.0",
    "@storybook/addon-svelte-csf": "^3.0.10",
    "@storybook/blocks": "^7.4.0",
    "@storybook/svelte": "^7.4.0",
    "@storybook/sveltekit": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^7.4.0",
    "@sveltejs/adapter-vercel": "^3.0.3",
    "@sveltejs/kit": "^1.24.1",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "^20.5.9",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-svelte": "^2.33.0",
    "firebase": "^10.3.1",
    "firebase-svelte": "^1.0.1",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.29",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.66.1",
    "storybook": "^7.4.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "svelte-i18n": "^3.7.4",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "zod": "^3.22.2"
  },
  "type": "module"
}
